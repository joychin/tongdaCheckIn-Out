{
  "name": "承道打卡",
  "version": "1.3",
  "description": "承道自动打卡",
  "permissions": [ "background"],

  "background": { "scripts": ["common.js", "background.js"] },

  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },

  "browser_action": {
    "default_icon": "clock-19.png",
    "default_title": "OA打卡",
    "default_popup": "popup.html"
  },

  "icons": {
    "16": "clock-16.png",
    "48": "clock-48.png",
    "128": "clock-128.png",
    "256": "clock-256.png"
  },

  "manifest_version": 2
}
